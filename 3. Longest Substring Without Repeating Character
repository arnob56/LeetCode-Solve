class Solution:
    def lengthOfLongestSubstring(self, s: str) -> int:
        subStr=[]
        mx_len=0

        for ch in s:
            while ch in subStr:
                subStr.pop(0)   #rmv first charactter
            subStr.append(ch)    # add current char

            if len(subStr) > mx_len:
                mx_len = len(subStr)
        return mx_len
