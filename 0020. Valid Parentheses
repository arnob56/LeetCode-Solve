class Solution:
    def isValid(self, s: str) -> bool:
        stack = []

        for ch in s:


            if ch == '(' or ch == '[' or ch == '{':
                stack.append(ch)

            else:

                if len(stack) == 0:
                    return False

                last = stack.pop()

                if ch == ')' and last != '(':
                    return False
                if ch == ']' and last != '[':
                    return False
                if ch == '}' and last != '{':
                    return False

        if len(stack) == 0:
            return True
        else:
            return False
